{
  "private": true,
  "name": "eticaretbackendnode",
  "packageManager": "yarn@3.3.1",
  "workspaces": [
    "services/*"
  ],
  "scripts": {
     "prestart": "cp scripts/pre-commit .git/hooks/ && chmod +x .git/hooks/pre-commit && echo 'hook copied'",
     "prestart:windows": "powershell 'Copy-Item scripts/pre-commit .git/hooks/'  && echo 'hook copied'"
  }
}
